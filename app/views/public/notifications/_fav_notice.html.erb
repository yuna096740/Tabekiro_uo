<%= link_to 'あなたの投稿', post_path(notice.post) %>
<p>にいいねしました</p>
<div class="text-secondary ml-3">
  <%= time_ago_in_words(notice.created_at).upcase %>
  <%= link_to notification_path(notice), method: :delete, remote: :true, class:"text-dark ml-3" do %>
    <i class="fas fa-trash"></i>
  <% end %>
</div>